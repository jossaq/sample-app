[Unit]
Description=uWSGI instance to serve sampla app
After=network.target

[Service]
User=ec2-user
Group=nginx
WorkingDirectory=/home/ec2-user/sample-app
Environment="PATH=/home/ec2-user/sample-app/.venv/bin"
ExecStart=/home/ec2-user/sample-app/.venv/bin/uwsgi --ini usgi.ini
PrivateTmp=No

[Install]
WantedBy=multi-user.target